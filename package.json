{
  "name": "@indexed-finance/uniswap-v2-oracle",
  "version": "1.0.6",
  "description": "",
  "main": "buidler.config.js",
  "files": [
    "contracts",
    "artifacts",
    "deployments/rinkeby",
    "deployments/mainnet",
    "deployments/polygon"
  ],
  "scripts": {
    "clean:build": "rimraf ./artifacts && rimraf ./cache && npm run clean:temp",
    "clean:temp": "buidler-verify-helper --clean",
    "prepare-build": "npm run clean:build && buidler-verify-helper --prepare",
    "prepare:oracle": "yarn prepare-build --contracts IndexedUniswapV2Oracle",
    "deploy:oracle": "yarn prepare:oracle && buidler deploy --tags Oracle --network",
    "build": "buidler compile",
    "coverage": "buidler coverage --network coverage --solcoverjs ./.solcover.js",
    "test": "buidler --network polygon run scripts/test.js"
  },
  "keywords": [],
