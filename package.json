    "bn.js": "^5.1.3",
    "buidler-deploy": "^0.6.0-beta.34",
    "buidler-ethers-v5": "^0.2.3",
    "buidler-verify-helper": "^1.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
